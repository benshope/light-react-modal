{
  "name": "minimalist-react-modal",
  "version": "0.0.4",
  "description": "Minimalist React Modal",
  "main": "dist/index.js",
  "files": ["dist"],
  "scripts": {
    "start": "start-storybook -p 9001",
    "test": "jest",
    "build": "yarn build:src && yarn build:flow",
    "build:src": "./node_modules/.bin/webpack --mode=production",
    "build:flow": "flow-copy-source -v src dist",
    "build:storybook": "rm -rf docs && mkdir docs && build-storybook -c .storybook -o docs",
    "set-registry": "perl -pi -e 's/unpm.uberinternal.com/registry.yarnpkg.com/g' `find . -name yarn.lock`",
    "publish-patch": "yarn set-registry && git push --tags && yarn build && yarn publish"
  },
  "author": "Ben Shope",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^4",
    "@storybook/addon-actions": "^4",
    "@storybook/addon-info": "^4",
    "@storybook/addon-options": "^4",
    "@storybook/addon-storyshots": "^4",
    "@storybook/react": "^4",
    "babel-core": "6",
    "babel-loader": "^8.0.0-beta",
    "flow-bin": "^0.89.0",
    "flow-copy-source": "^2.0.2",
    "react": "16.8.0-alpha.0",
    "react-dom": "16.8.0-alpha.0",
    "styled-components": "^4.1.3",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1"
  }
}
