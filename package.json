{
    "name": "light-react-modal",
    "version": "0.0.5",
    "description": "Light React Modal",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "deploy": "build-storybook -o docs && storybook-to-ghpages",
        "start": "npm run storybook",
        "storybook": "start-storybook -p 9001",
        "test": "jest"
    },
    "author": "Ben Shope",
    "license": "MIT",
    "dependencies": {
        "prop-types": "^15.7.2"
    },
    "peerDependencies": {
        "react": "^16.8.0-alpha.0",
        "react-dom": "^16.8.0-alpha.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@storybook/addon-a11y": "^5",
        "@storybook/addon-actions": "^5",
        "@storybook/addon-info": "^5",
        "@storybook/addon-options": "^5",
        "@storybook/addon-storyshots": "^5",
        "@storybook/react": "^5",
        "@storybook/storybook-deployer": "^2.8.1",
        "babel-loader": "^8.0.5",
        "eslint": "^5.15.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "prettier": "1.16.4",
        "react": "^16.8",
        "react-dom": "^16.8"
    },
    "esLintConfig": {
        "env": {
            "browser": true,
            "commonjs": true,
            "es6": true
